{
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "checkJs": true,
    "diagnostics": true,
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true
  },
  "exclude": ["node_modules/*"],
  "typeAcquisition": {}
}
